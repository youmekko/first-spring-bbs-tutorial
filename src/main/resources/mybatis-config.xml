<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
 PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
 "http://mybatis.org//dtd/mybatis-3-config.dtd">
<configuration>

	<!-- typeAliases는 하위요소로 package와 typeAlias 요소를 가질 수 있다. -->
	<!-- 이 요소들을 이용하면 매번 parameterType이나 resultType에 사용하는 클래스의 이름을 org.zerock.domain은 
		생략한 채로 표현하는 것이 가능하다. -->
	<typeAliases>
		<package name="org.zerock.domain" />
	</typeAliases>

</configuration>